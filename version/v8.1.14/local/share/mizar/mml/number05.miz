begin :: Problem 12

theorem Th1:
  for n,k being Nat st n|^|^k = 0 holds n=0
  proof
    let n,k be Nat;
    assume A1: n|^|^k = 0;
    then k <> 0 by ORDINAL5:13;
    then consider j being Nat such that
    A2: k=j+1 by NAT_1:6;
    n|^|^(j+1) = n|^|^(Segm (j+1))
    .= n|^|^(succ Segm j) by NAT_1:38
    .= exp(n,n|^|^j) by ORDINAL5:14
    .= n|^(n|^|^j) by ORDINAL5:27;
    hence n=0 by A1,A2;
  end;

registration
  let x be odd Nat;
  let i be Nat;
  cluster x|^|^i -> odd;
  coherence
  proof
    defpred P[Nat] means x|^|^$1 is odd;
    A1: P[0] by ORDINAL5:13,POLYFORM:4;
    A2:
    now
      let k be Nat;
      assume P[k];
      x|^|^(k+1) = x|^|^(Segm (k+1))
      .= x|^|^(succ Segm k) by NAT_1:38
      .= exp(x,x|^|^k) by ORDINAL5:14
      .= x|^(x|^|^k) by ORDINAL5:27;
      hence P[k+1] by PEPIN:20;
    end;
    for k being Nat holds P[k] from NAT_1:sch 2(A1,A2);
    hence x|^|^i is odd;
  end;
end;

registration
  let x be non zero even Nat;
  let i be non zero Nat;
  cluster x|^|^i -> even;
  coherence
  proof
    consider j being Nat such that
    A1: i=j+1 by NAT_1:6;
    A2: x|^|^(j+1) = x|^|^(Segm (j+1))
    .= x|^|^(succ Segm j) by NAT_1:38
    .= exp(x,x|^|^j) by ORDINAL5:14
    .= x|^(x|^|^j) by ORDINAL5:27;
    per cases;
    suppose x|^|^j > 0;
      hence x|^|^i is even by A2,A1;
    end;
    suppose x|^|^j = 0;
      hence thesis by Th1;
    end;
  end;
end;

:: Problem 12
theorem
  for n being non zero Nat ex x being non zero Nat st
  for i being Nat holds n divides x|^|^(i+1) + 1
  proof
    let n be non zero Nat;
    A1: n divides 2*n & 2 divides 2*n;
    set x=2*n-1;
    reconsider x as non zero odd Nat;
    take x;
    let i be Nat;

    A3: x|^|^((i)+1) = x|^|^(Segm((i)+1))
    .= x|^|^(succ Segm(i)) by NAT_1:38
    .= exp(x,x|^|^(i)) by ORDINAL5:14
    .= x|^(x|^|^(i)) by ORDINAL5:27;

    2*n divides x - -1; then
    x|^(x|^|^(i)), (-1)|^(x|^|^(i)) are_congruent_mod 2*n
    by GR_CY_3:34,INT_1:def 4;
    then x|^|^(i+1),-1 are_congruent_mod 2*n by A3;
    then 2*n divides x|^|^(i+1) - -1;
    hence thesis by A1,INT_2:9;
  end;

begin :: Problem 13

theorem Th3:
  for n,k being Nat st n=4*k+3 ex p,q being Nat st
  p=4*q+3 & p is prime & p divides n
  proof
    defpred P[Nat] means
    (ex k being Nat st $1=4*k+3) implies
    ex p,q being Nat st
    p=4*q+3 & p is prime & p divides $1;
    let n,k be Nat such that
    A1: n=4*k+3;
    A2: for m being Nat st for l being Nat st l < m holds P[l] holds P[m]
    proof
      let m be Nat;
      assume A3: for l being Nat st l < m holds P[l];
      given k being Nat such that
      A4: m=4*k+3;
      m <> 0 & m <> 1 by A4,NAT_1:11;
      then m is non trivial by NAT_2:28;
      then consider l being prime Nat such that
      A5: l divides m by NEWTON03:29;
      reconsider l as Element of NAT by ORDINAL1:def 12;
      consider t being Nat such that
      A6: m = l*t by A5,NAT_D:def 3;
      A7: t divides m by A6;
      consider u being Element of NAT such that
      A8: l=4*u or l=4*u+1 or l=4*u+2 or l=4*u+3 by SCHEME1:3;
      per cases by A8;
      suppose l=4*u;
        then 4 divides l;
        then 4 divides m by A5,NAT_D:4;
        hence ex p,q being Nat st
        p=4*q+3 & p is prime & p divides m by A4,NUMBER02:32;
      end;
      suppose A9: l=4*u+1;
        reconsider t as Element of NAT by ORDINAL1:def 12;
        consider v being Element of NAT such that
        A10: t=4*v or t=4*v+1 or t=4*v+2 or t=4*v+3 by SCHEME1:3;
        per cases by A10;
        suppose t=4*v;
          then 4 divides t;
          then 4 divides m by A7,NAT_D:4;
          hence ex p,q being Nat st
          p=4*q+3 & p is prime & p divides m by A4,NUMBER02:32;
        end;
        suppose t=4*v+1;
          then m=4*(4*u*v+u+v)+1 by A6,A9;
          then A11: m mod 4 = 1 mod 4 by NAT_D:21
          .= 1 by NAT_D:24;
          m mod 4 = 3 mod 4 by A4,NAT_D:21
          .= 3 by NAT_D:24;
          hence ex p,q being Nat st
          p=4*q+3 & p is prime & p divides m by A11;
        end;
        suppose t=4*v+2;
          then t=2*(2*v+1);
          then A12: 2 divides t;
          m=2*(2*k+1)+1 by A4;
          hence ex p,q being Nat st
          p=4*q+3 & p is prime & p divides m by A12,A7;
        end;
        suppose A13: t=4*v+3;
          A14: t <= m by A6,NAT_D:def 3,INT_2:27;
          now
            assume t=m;
            then l=1 by A6,A4,XCMPLX_1:7;
            hence contradiction by INT_2:def 4;
          end; then
          t < m by A14,XXREAL_0:1;
          then consider p,q being Nat such that
          A15: p=4*q+3 & p is prime & p divides t by A13,A3;
          take p,q;
          thus p=4*q+3 & p is prime & p divides m by A15,A7,NAT_D:4;
        end;
      end;
      suppose l=4*u+2;
        then l=2*(2*u+1);
        then A16: 2 divides l;
        m=2*(2*k+1)+1 by A4;
        hence ex p,q being Nat st
        p=4*q+3 & p is prime & p divides m by A16,A5;
      end;
      suppose l=4*u+3;
        hence ex p,q being Nat st
        p=4*q+3 & p is prime & p divides m by A5;
      end;
    end;
    for n being Nat holds P[n] from NAT_1:sch 4(A2);
    then consider p,q being Nat such that
    A17: p=4*q+3 & p is prime & p divides n by A1;
    take p,q;
    thus thesis by A17;
  end;

definition
  defpred P[set] means ex k being Nat st $1=4*k+3 & 4*k+3 is prime;
  func 4k+3_Primes -> Subset of NAT means :Def1:
  for n being Nat holds n in it iff ex k being Nat st n=4*k+3 & n is prime;
  existence
  proof
    consider s being Subset of NAT such that
    A1: for x being set holds x in s iff x in NAT & P[x] from SUBSET_1:sch 1;
    take s;
    let n be Nat;
    thus n in s implies ex k being Nat st n=4*k+3 & n is prime
    proof
      assume n in s;
      then consider k being Nat such that
      A2: n=4*k+3 & 4*k+3 is prime by A1;
      thus thesis by A2;
    end;
    given k being Nat such that A3: n=4*k+3 & n is prime;
    thus n in s by A3,A1;
  end;
  uniqueness
  proof
    let s1,s2 be Subset of NAT such that
    A4: for n being Nat holds n in s1 iff
    ex k being Nat st n=4*k+3 & n is prime
    and
    A5: for n being Nat holds n in s2 iff
    ex k being Nat st n=4*k+3 & n is prime;
    A6: for n being Element of NAT holds n in s1 iff P[n]
    proof
      let n be Element of NAT;
      thus n in s1 implies P[n]
      proof
        assume n in s1;
        then consider k being Nat such that
        A7: n=4*k+3 & n is prime by A4;
        thus P[n] by A7;
      end;
      assume P[n];
      then consider k being Nat such that
      A8: n=4*k+3 & 4*k+3 is prime;
      thus n in s1 by A8,A4;
    end;
    A9: for n being Element of NAT holds n in s2 iff P[n]
    proof
      let n be Element of NAT;
      thus n in s2 implies P[n]
      proof
        assume n in s2;
        then consider k being Nat such that
        A10: n=4*k+3 & n is prime by A5;
        thus P[n] by A10;
      end;
      assume P[n];
      then consider k being Nat such that
      A11: n=4*k+3 & 4*k+3 is prime;
      thus n in s2 by A11,A5;
    end;
    thus s1=s2 from SUBSET_1:sch 2(A6,A9);
  end;
end;

theorem Th4:
  for n being Nat st n in 4k+3_Primes holds n >= 3
  proof
    let n be Nat;
    assume n in 4k+3_Primes;
    then consider k being Nat such that
    A1: n=4*k+3 & n is prime by Def1;
    thus n >= 3 by A1,NAT_1:11;
  end;

registration
  cluster 4k+3_Primes -> infinite;
  coherence
  proof
    3=4*0+3 & 3 is prime by PEPIN:41; then
    A1: 3 in 4k+3_Primes by Def1;
    assume 4k+3_Primes is finite;
    then consider n being Element of NAT such that
    A2: 4k+3_Primes c= Seg n \/ {0} by A1,HEYTING3:1;
    A3: 4k+3_Primes c= Seg n by A2,ZFMISC_1:135,Th4;
    set X=4k+3_Primes\{3};
    X c= 4k+3_Primes by XBOOLE_1:36;
    then A4: X c= Seg n by A3,XBOOLE_1:1;
    set N=4*(Product Sgm X) + 3;
    consider p,q being Nat such that
    A5: p=4*q+3 & p is prime & p divides N by Th3;
    A6: p in 4k+3_Primes by Def1,A5;
    rng Sgm X c= SetPrimes
    proof
      let a be object;
      assume A7: a in rng Sgm X;
      then a in X by A4,FINSEQ_1:def 13;
      then A8: a in 4k+3_Primes by XBOOLE_0:def 5;
      reconsider n=a as Nat by A7;
      consider k being Nat such that
      A9: n=4*k+3 & n is prime by A8,Def1;
      thus a in SetPrimes by A9,NEWTON:def 6;
    end;
    then reconsider SX=Sgm X as FinSequence of SetPrimes by FINSEQ_1:def 4;
    for p being prime Nat st p in 4k+3_Primes holds not p divides N
    proof
      let p be prime Nat;
      assume A10: p in 4k+3_Primes;
      assume A11: p divides N;
      p >= 3 by A10,Th4;
      then per cases by XXREAL_0:1;
      suppose A12: p=3;
        2,3 are_coprime by INT_2:30,INT_2:28,PEPIN:41;
        then A13: 2*2,3 are_coprime by EULER_1:14;
        p divides 4*(Product Sgm X) by A11,A12,INT_2:1;
        then p in rng SX by A12,A13,EULER_1:13,NAT_3:8;
        then p in X by A4,FINSEQ_1:def 13;
        then not p in {3} by XBOOLE_0:def 5;
        hence contradiction by A12,TARSKI:def 1;
      end;
      suppose A14: p>3;
        then not p in {3} by TARSKI:def 1;
        then p in X by A10,XBOOLE_0:def 5;
        then p in rng SX by A4,FINSEQ_1:def 13;
        then p divides 4*Product Sgm X by NAT_D:9,NAT_3:7;
        hence contradiction by A14,INT_2:1,27,A11;
      end;
      end;
    hence contradiction by A5,A6;
  end;
end;

:: Problem 13
theorem
  for n being Nat st n in 4k+3_Primes
  for x being even Nat
  for i being Nat holds not n divides x|^|^(i+2) + 1
  proof
    let n be Nat;
    assume A1: n in 4k+3_Primes;
    then A2: n >= 3 by Th4;
    let x be even Nat;
    let i be Nat;
    reconsider i2=i+2 as Nat;
    consider m being Element of NAT such that
    A3: i2 = 2*m or i2 = 2*m+1 by SCHEME1:1;
    per cases;
    suppose A4: x is zero;
      per cases by A3;
      suppose i+2=2*m;
        then A5: x|^|^(i+2) = 1 by A4,ORDINAL5:20;
        assume n divides x|^|^(i+2) + 1;
        then n <= 2 by INT_2:27,A5;
        hence contradiction by A2,XXREAL_0:2;
      end;
      suppose i+2=2*m+1;
        then A6: x|^|^(i+2) = 0 by A4,ORDINAL5:20;
        assume n divides x|^|^(i+2) + 1;
        then n <= 1 by INT_2:27,A6;
        hence contradiction by A2,XXREAL_0:2;
      end;
    end;
    suppose x is non zero;
      then consider m being Nat such that
      A7: x|^|^(i+1) = 2*m by ABIAN:def 2;
      A8: x|^|^((i+1)+1) = x|^|^(Segm((i+1)+1))
      .= x|^|^(succ Segm(i+1)) by NAT_1:38
      .= exp(x,x|^|^(i+1)) by ORDINAL5:14
      .= x|^(m*2) by A7,ORDINAL5:27
      .= (x|^m)|^2 by NEWTON:9;
      assume n divides x|^|^(i+2) + 1;
      then (x|^m)|^2 -  -1 mod n = 0 by A8,PEPIN:6,A2;
      then A9: (x|^m)^2 -  -1 mod n = 0 by WSIERP_1:1;
      consider k being Nat such that
      A10: n=4*k+3 & n is prime by A1,Def1;
      A11: n mod 4 = 3 mod 4 by A10,NAT_D:21
      .= 3 by NAT_D:24;
      n > 2 by A2,XXREAL_0:2;
      hence contradiction by A11,A9,INT_5:def 2,A10,INT_5:38;
    end;
  end;

begin :: Problem 31

theorem Th6:
  for a being Integer st not 3 divides a holds a|^3 mod 9 = 1 or a|^3 mod 9 = 8
  proof
    let a be Integer;
    assume not 3 divides a;
    then a mod 3 <> 0 by INT_1:62;
    then a mod 3 > 0 & a mod 3 < 3 by INT_1:57,58;
    then A1: a mod 3 > 0 & a mod 3 in Segm 3 by NAT_1:44;
    set d = a div 3;
    A2:
    now
      assume a mod 3 = 1;
      then a = d*3 + 1 by INT_1:59;
      then a|^3 = (3*d)|^3+3*(3*d)|^2+3*(3*d)+1 by SERIES_2:2
      .= (3|^3*d|^3)+3*(3*d)|^2+3*(3*d)+1 by NEWTON:7
      .= ((3*3*3)*d|^3)+3*(3*d)|^2+3*(3*d)+1 by POLYEQ_3:27
      .= ((3*3*3)*d|^3)+3*((3|^2*d|^2))+3*(3*d)+1 by NEWTON:7
      .= ((3*3*3)*d|^3)+3*((3*3)*d|^2)+3*(3*d)+1 by WSIERP_1:1
      .= 9*(3*d|^3+3*d|^2+d)+1;
      hence a|^3 mod 9 = ((9*(3*d|^3+3*d|^2+d) mod 9) + (1 mod 9)) mod 9
      by EULER_2:6
      .= (0 + (1 mod 9)) mod 9 by NAT_D:71
      .= 1 by NAT_D:63;
    end;
    A3:
    now
      assume a mod 3 = 2;
      then a = d*3 + 2 by INT_1:59;
      then a|^3 = (3*d)|^3+3*(3*d)|^2*2+3*2|^2*(3*d)+2|^3 by SERIES_4:2
      .= (3|^3*d|^3)+3*(3*d)|^2*2+3*2|^2*(3*d)+2|^3 by NEWTON:7
      .= ((3*3*3)*d|^3)+3*(3*d)|^2*2+3*2|^2*(3*d)+2|^3 by POLYEQ_3:27
      .= ((3*3*3)*d|^3)+3*(3|^2*d|^2)*2+3*2|^2*(3*d)+2|^3 by NEWTON:7
      .= ((3*3*3)*d|^3)+3*((3*3)*d|^2)*2+3*2|^2*(3*d)+2|^3 by WSIERP_1:1
      .= 9*(3*d|^3+3*d|^2*2+2|^2*d)+(2*2*2) by POLYEQ_3:27;
      hence a|^3 mod 9 = ((9*(3*d|^3+3*d|^2*2+2|^2*d)) mod 9 +
      ((2*2*2) mod 9)) mod 9 by EULER_2:6
      .= (0 + (8 mod 9)) mod 9 by NAT_D:71
      .= 8 by NAT_D:63;
    end;
    thus thesis by A2,A3,A1,ENUMSET1:def 1,CARD_1:51;
  end;

:: Problem 31
theorem
  for a,b,c being Integer st 9 divides a|^3+b|^3+c|^3 holds
  3 divides a or 3 divides b or 3 divides c
  proof
    let a,b,c be Integer;
    assume 9 divides a|^3+b|^3+c|^3;
    then A1: a|^3+b|^3+c|^3 mod 9 = 0 by INT_1:62;
    assume not 3 divides a & not 3 divides b & not 3 divides c;
    then per cases by Th6;
    suppose A2: a|^3 mod 9 = 1 & b|^3 mod 9 = 1 & c|^3 mod 9 = 1;
      ((a|^3+b|^3)+c|^3) mod 9 = (((a|^3+b|^3) mod 9) + (c|^3 mod 9)) mod 9
      by EULER_2:6
      .= ((((a|^3 mod 9) + (b|^3 mod 9)) mod 9) + (c|^3 mod 9)) mod 9
      by EULER_2:6
       .= (2 + 1) mod 9 by A2,NAT_D:63;
      hence contradiction by A1,NAT_D:63;
    end;
    suppose A3: a|^3 mod 9 = 1 & b|^3 mod 9 = 1 & c|^3 mod 9 = 8;
      ((a|^3+b|^3)+c|^3) mod 9 = (((a|^3+b|^3) mod 9) + (c|^3 mod 9)) mod 9
      by EULER_2:6
      .= ((((a|^3 mod 9) + (b|^3 mod 9)) mod 9) + (c|^3 mod 9)) mod 9
      by EULER_2:6
      .= (2+8) mod 9 by A3,NAT_D:63
      .= (1+9) mod 9
      .= ((1 mod 9) + (9 mod 9)) mod 9 by EULER_2:6
      .= ((1 mod 9) + 0) mod 9 by INT_1:50;
      hence contradiction by A1,NAT_D:63;
    end;
    suppose A4: a|^3 mod 9 = 1 & b|^3 mod 9 = 8 & c|^3 mod 9 = 1;
      ((a|^3+b|^3)+c|^3) mod 9 = (((a|^3+b|^3) mod 9) + (c|^3 mod 9)) mod 9
      by EULER_2:6
      .= ((((a|^3 mod 9) + (b|^3 mod 9)) mod 9) + (c|^3 mod 9)) mod 9
      by EULER_2:6
      .= (0+1) mod 9 by A4,INT_1:50;
      hence contradiction by A1,NAT_D:63;
    end;
    suppose A5: a|^3 mod 9 = 1 & b|^3 mod 9 = 8 & c|^3 mod 9 = 8;
      ((a|^3+b|^3)+c|^3) mod 9 = (((a|^3+b|^3) mod 9) + (c|^3 mod 9)) mod 9
      by EULER_2:6
      .= ((((a|^3 mod 9) + (b|^3 mod 9)) mod 9) + (c|^3 mod 9)) mod 9
      by EULER_2:6
      .= (0+8) mod 9 by A5,INT_1:50;
      hence contradiction by A1,NAT_D:63;
    end;
    suppose A6: a|^3 mod 9 = 8 & b|^3 mod 9 = 1 & c|^3 mod 9 = 1;
      ((a|^3+b|^3)+c|^3) mod 9 = (((a|^3+b|^3) mod 9) + (c|^3 mod 9)) mod 9
      by EULER_2:6
      .= ((((a|^3 mod 9) + (b|^3 mod 9)) mod 9) + (c|^3 mod 9)) mod 9
      by EULER_2:6
      .= (0+1) mod 9 by A6,INT_1:50;
      hence contradiction by A1,NAT_D:63;
    end;
    suppose A7: a|^3 mod 9 = 8 & b|^3 mod 9 = 1 & c|^3 mod 9 = 8;
      ((a|^3+b|^3)+c|^3) mod 9 = (((a|^3+b|^3) mod 9) + (c|^3 mod 9)) mod 9
      by EULER_2:6
      .= ((((a|^3 mod 9) + (b|^3 mod 9)) mod 9) + (c|^3 mod 9)) mod 9
      by EULER_2:6
      .= (0+8) mod 9 by A7,INT_1:50;
      hence contradiction by A1,NAT_D:63;
    end;
    suppose A8: a|^3 mod 9 = 8 & b|^3 mod 9 = 8 & c|^3 mod 9 = 1;
      ((a|^3+b|^3)+c|^3) mod 9 = (((a|^3+b|^3) mod 9) + (c|^3 mod 9)) mod 9
      by EULER_2:6
