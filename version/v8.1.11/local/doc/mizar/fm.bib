    @ARTICLE{NOMIN_1.ABS,
    JOURNAL = {Formalized Mathematics},
	ISSN = {1426-2630},
	EISSN = {1898-9934},
      VOLUME = {25},
    NUMBER = {{\bf 3}},
    PAGES = {205--216},
