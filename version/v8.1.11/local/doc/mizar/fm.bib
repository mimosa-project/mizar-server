    SECTION2 = {Definition of Simple-Named Complex-Valued Nominative Data},
    SECTION3 = {Examples of Simple-Named Complex-Valued Nominative Data},
    SECTION4 = {Operations on Simple-Named Complex-Valued Nominative Data},
