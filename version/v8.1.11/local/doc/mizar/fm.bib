            SECTION4 = {Eucldiean Plane Satisfies First 7 Tarski's Axioms},
            SECTION5 = {Preparation for The Rest of Tarski's Axioms},
            SECTION6 = {Four Remaining Axioms of Tarski},
            SECTION7 = {Axiom A11 -- Axiom Schema of Continuity},
